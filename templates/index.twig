{% extends 'master.twig' %}
{% block title %}Home{% endblock %}

{% block content %}
	<div class="table-responsive">
		<table class="table table-bordered table-hover table-striped">
			<thead>
				<tr>
					<th>Name</th>
					<th>Email</th>
				</tr>
			</thead>
			<tbody>
				{% for contact in contacts %}
				    <tr>
				    	<td>{{ contact.name }}</td>
				    	<td>{{ contact.email }}</td>
				    </tr>
				{% endfor %}
			</tbody>
		</table>
	</div>
{% endblock %}